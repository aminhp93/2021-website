        "version": "detect"
      }
    },
    "plugins": [
      "@typescript-eslint",
      "react"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "ignorePatterns": [
      "node_modules",
      "dist",
      "__mocks__",
      "__tests__",
      "coverage",
      "lib",
      "build.js",
      "jest.config.js"
    ],
    "rules": {
      "no-empty-function": 1,
      "@typescript-eslint/no-empty-function": 1,
      "react/prop-types": 1,
      "react/no-deprecated": 1,
      "react/no-find-dom-node": 1
    }
  },
  "eslint-teamcity": {
    "reporter": "inspections",
